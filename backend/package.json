{
  "name": "golding-backend",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "seed": "ts-node --project tsconfig.seed.json prisma/seed.ts"
  },
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon -r ts-node/register/transpile-only src/server.ts",
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "NODE_ENV=test DATABASE_URL=\"postgresql://postgres:postgres@localhost:5433/golding_test\" jest --detectOpenHandles --forceExit",
    "test:watch": "NODE_ENV=test DATABASE_URL=\"postgresql://postgres:postgres@localhost:5433/golding_test\" jest --watch",
    "test:coverage": "NODE_ENV=test DATABASE_URL=\"postgresql://postgres:postgres@localhost:5433/golding_test\" jest --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "DATABASE_URL=\"postgresql://postgres:postgres@localhost:5433/golding_test\" prisma migrate deploy",
    "prisma:seed": "prisma db seed",
    "prisma:reset": "prisma migrate reset --force",
    "prisma:studio": "prisma studio",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^6.3.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.0.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.17.16",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^6.3.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
